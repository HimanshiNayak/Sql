//create database

CREATE DATABASE  name;
USE name;

//create table

CREATE TABLE student(
id INT PRIMARY KEY,
name varchar(50),
age INT NOT NULL
);

//insert values
//single values
INSERT INTO student VALUES(1,"abc",20);
INSERT INTO student VALUES(2,"bcd",21);
INSERT INTO student VALUES(3,"def",22);

//multiple values 

INSERT INTO student (id , name,age) VALUES(1,"abc",20) , (2,"bcd",21) ,(3,"def",22);
//display

SELECT * FROM student;
